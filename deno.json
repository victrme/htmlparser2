{
  "name": "@victr/htmlparser2",
  "version": "0.0.0",
  "description": "Fast & forgiving HTML/XML parser, on JSR.io",
  "keywords": [
    "html",
    "parser",
    "streams",
    "xml",
    "dom",
    "rss",
    "feed",
    "atom"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/victrme/htmlparser2.git"
  },
  "license": "MIT",
  "author": "Felix Boehm <me@feedic.com>",
  "exports": "./src/index.ts",
  "files": ["src"],
  "exclude": ["src/*.spec.ts", "src/__fixtures__"],
  "scripts": {
    "format": "npm run format:es && npm run format:prettier",
    "format:es": "npm run lint:es -- --fix",
    "format:prettier": "npm run format:prettier:raw -- --write",
    "format:prettier:raw": "prettier '**/*.{ts,md,json,yml}'",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:prettier",
    "lint:es": "eslint src",
    "lint:prettier": "npm run format:prettier:raw -- --check",
    "lint:ts": "tsc --noEmit",
    "test": "npm run test:vi && npm run lint",
    "test:vi": "vitest run"
  },
  "fmt": {
    "tabWidth": 4
  },
  "imports": {
    "domelementtype": "npm:domelementtype@^2.3.0",
    "domhandler": "npm:domhandler@^5.0.3",
    "domutils": "npm:domutils@^3.1.0",
    "entities": "npm:entities@^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.13.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.0.2"
  }
}
